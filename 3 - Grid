// Random central square with a size that is a multiple of 50. 10x10 grid.

  int tamanho; 
  int x = 0;
  int y = 0;

 

void setup()
{
  size(600,600);
 
  tamanho = 400+50* (int) random (5);
 
  print(tamanho);
  

}

â€‹

void draw()
{
  background(0);
  
  fill(255);
    rect(x,y,tamanho,tamanho);
    
  for(int i = 0; i < tamanho; i += (tamanho/10))
  {
    
    for(int j = 0; j < tamanho; j += (tamanho/10))
    {
          
      rect(i,j,tamanho/10, tamanho/10);
    }
  }
  


}
